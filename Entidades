using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ET
{
    public class ET_Solicitud
    {
        private int _id;
        private string _idUsuario;
        private string _idTipoSolicitud;
        private string _asunto;
        private string _descripcionSolicitud;
        private bool _Estado;
        private DateTime _Fecha;

        public int Id { get => _id; set => _id = value; }
        public string IdUsuario { get => _idUsuario; set => _idUsuario = value; }
        public string IdTipoSolicitud { get => _idTipoSolicitud; set => _idTipoSolicitud = value; }
        public string Asunto { get => _asunto; set => _asunto = value; }
        public string DescripcionSolicitud { get => _descripcionSolicitud; set => _descripcionSolicitud = value; }
        public bool Estado { get => _Estado; set => _Estado = value; }
        public DateTime Fecha { get => _Fecha; set => _Fecha = value; }
    }
}


///////////////////////////////////REPORTE_ET////////////////////////////////////////////////////

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ET
{
    public class ET_ReportePaga
    {
        private int _id;
        private string _idEmpleado;
        private DateTime _Fecha;
        private decimal _salarioBase;
        private decimal _bonos;
        private decimal _deducciones;
        private decimal _impuestos;
        private decimal _salarioNeto;

        public int Id { get => _id; set => _id = value; }
        public string IdEmpleado { get => _idEmpleado; set => _idEmpleado = value; }
        public DateTime Fecha { get => _Fecha; set => _Fecha = value; }
        public decimal SalarioBase { get => _salarioBase; set => _salarioBase = value; }
        public decimal Bonos { get => _bonos; set => _bonos = value; }
        public decimal Deducciones { get => _deducciones; set => _deducciones = value; }
        public decimal Impuestos { get => _impuestos; set => _impuestos = value; }
        public decimal SalarioNeto { get => _salarioNeto; set => _salarioNeto = value; }
    }
}

///////////////////////////////////MARCAS_ET////////////////////////////////////////////////////

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ET
{
    public class ET_Marcas
    {
        private int _id;
        private int _idUsuario;
        private DateTime _hora;
        private bool _entrada;
        private DateTime _fecha;
        private int _idPuesto;

        public int Id { get => _id; set => _id = value; }
        public intstring IdUsuario { get => _idUsuario; set => _idUsuario = value; }
        public DateTime Hora { get => _hora; set => _hora = value; }
        public bool Entrada { get => _entrada; set => _entrada = value; }
        public DateTime Fecha { get => _fecha; set => _fecha = value; }
        public int IdPuesto { get => _idPuesto; set => _idPuesto = value; }
    }
}

